<script>
function getRandomNewNumber(n) {

    var arr = new Array(n);
    var last = -1;

    for (var i = 0; i < n; i++) {
      arr[i] = i;
    }

    try {
      last = window.localStorage.getItem('review-slide') || -1;
    } catch (err) {
      // Ignore.
    }

    arr = arr.filter(function (a) { return a != last; });
    var next = arr[Math.floor(Math.random() * arr.length)];

    try {
      window.localStorage.setItem('review-slide', String(next));
    } catch (err) {
      // Ignore.
    }

    return next;

  }
 var total =  jQuery('.reviews-slider').find('.review-slide:not(.slick-cloned)').length;
 rand = getRandomNewNumber(total);
  
$('.reviews-slider').slick({
